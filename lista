def merge_intervals(intervalos):
  for index_i,i in enumerate(intervalos):
    for j in intervalos[index_i+1:]:
      if i[1]>=j[0]:
        if i[1]>j[1]:
          intervalos.remove(j)
        if i[1]<j[1]:
          i[1]=j[1]
          intervalos.remove(j)
  return intervalos

print(merge_intervals([[1, 6], [2, 3], [3, 9], [12, 28], [13, 32], [32, 47]]))
